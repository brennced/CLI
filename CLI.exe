Public Class ConfigWin
    Dim a
    Dim cc = True
    Private Sub Timer1_Tick(sender As Object, e As EventArgs) Handles UpdateTimer.Tick
        If iconCaps.Checked Then
            If My.Computer.Keyboard.CapsLock = True Then
                TaskbarIcon.Icon = My.Resources.caps_on
                TaskbarIcon.Text = "CapsLock Indicator: Capslock ON"
                TaskbarIcon.BalloonTipText = "Caps: ON"
            Else
                TaskbarIcon.Icon = My.Resources.caps_off
                TaskbarIcon.Text = "CapsLock Indicator: Capslock OFF"
                TaskbarIcon.BalloonTipText = "Caps: OFF"
            End If
            If Not My.Computer.Keyboard.CapsLock = a Then
                If ShowBaloonToolStripMenuItem.Checked Then
                    TaskbarIcon.ShowBalloonTip(1)
                End If
            End If
            a = My.Computer.Keyboard.CapsLock
            My.Settings.Mode = 0
        ElseIf iconNum.Checked Then
            If My.Computer.Keyboard.NumLock = True Then
                TaskbarIcon.Icon = My.Resources.num_on
                TaskbarIcon.Text = "CapsLock Indicator: Numlock ON"
                TaskbarIcon.BalloonTipText = "Num: ON"
            Else
                TaskbarIcon.Icon = My.Resources.num_off
                TaskbarIcon.Text = "CapsLock Indicator: Numlock OFF"
                TaskbarIcon.BalloonTipText = "Num: OFF"
            End If
            If Not My.Computer.Keyboard.NumLock = a Then
                If ShowBaloonToolStripMenuItem.Checked Then
                    TaskbarIcon.ShowBalloonTip(1)
                End If
            End If
            a = My.Computer.Keyboard.NumLock
            My.Settings.Mode = 1
        Else
            If My.Computer.Keyboard.ScrollLock = True Then
                TaskbarIcon.Icon = My.Resources.scroll_on
                TaskbarIcon.Text = "CapsLock Indicator: Scrolllock ON"
                TaskbarIcon.BalloonTipText = "Scroll: ON"
            Else
                TaskbarIcon.Icon = My.Resources.scroll_off
                TaskbarIcon.Text = "CapsLock Indicator: Scrolllock OFF"
                TaskbarIcon.BalloonTipText = "Scroll: OFF"
            End If
            If Not My.Computer.Keyboard.ScrollLock = a Then
                If ShowBaloonToolStripMenuItem.Checked Then
                    TaskbarIcon.ShowBalloonTip(1)
                End If
            End If
            a = My.Computer.Keyboard.ScrollLock
            My.Settings.Mode = 1
        End If

        If My.Computer.Keyboard.ShiftKeyDown AndAlso My.Computer.Keyboard.CtrlKeyDown Then
            Button3.Show()
            Button4.Show()
            Button5.Show()
        Else
            Button3.Hide()
            Button4.Hide()
            Button5.Hide()
        End If
    End Sub
    Private Sub CloseToolStripMenuItem_Click(sender As Object, e As EventArgs) Handles CLoseToolStripMenuItem.Click
        My.Settings.ShowBaloon = ShowBaloonToolStripMenuItem.Checked
        cc = False
        Me.Close()
    End Sub

    Private Sub ConfigWin_FormClosing(sender As Object, e As FormClosingEventArgs) Handles Me.FormClosing
        If cc Then
            Me.Hide()
        End If
        e.Cancel = cc
    End Sub
    Private Sub ConfigWin_Load(sender As Object, e As EventArgs) Handles MyBase.Load
        Me.Hide()
        ShowBaloonToolStripMenuItem.Checked = My.Settings.ShowBaloon
        CheckBox1.Checked = ShowBaloonToolStripMenuItem.Checked
        BootIcon.ShowBalloonTip(1)
        Label2.Text = "Version " & CLI.VersionPrefix & Application.ProductVersion & CLI.VersionSuffix
        If My.Settings.Mode = 0 Then
            iconCaps.Checked = True
        ElseIf My.Settings.Mode = 1 Then
            iconNum.Checked = True
        Else
            iconScroll.Checked = True
        End If
    End Sub

    Private Sub ShowBaloonToolStripMenuItem_Click(sender As Object, e As EventArgs) Handles ShowBaloonToolStripMenuItem.Click
        If ShowBaloonToolStripMenuItem.Checked Then
            ShowBaloonToolStripMenuItem.Checked = False
            CheckBox1.Checked = False
        Else
            ShowBaloonToolStripMenuItem.Checked = True
            CheckBox1.Checked = True
        End If
    End Sub

    Private Sub BootIconTimeout_Tick(sender As Object, e As EventArgs) Handles BootIconTimeout.Tick
        BootIconTimeout.Stop()
        BootIcon.Icon = Nothing
    End Sub

    Private Sub ShowConfigWindowToolStripMenuItem_Click(sender As Object, e As EventArgs) Handles ShowConfigWindowToolStripMenuItem.Click
        Me.ShowInTaskbar = True
        Me.Show()
        Me.WindowState = FormWindowState.Normal
        Me.CenterToScreen()
        Me.BringToFront()
        Me.Focus()
    End Sub

    Private Sub Button2_Click(sender As Object, e As EventArgs) Handles Button2.Click
        If MessageBox.Show("Do you really want to quit?", "CapsLock Indicator", MessageBoxButtons.YesNo, MessageBoxIcon.Question, MessageBoxDefaultButton.Button2) = Windows.Forms.DialogResult.Yes Then
            cc = False
            Me.Close()
        End If
    End Sub

    Private Sub Button1_Click(sender As Object, e As EventArgs) Handles Button1.Click
        cc = True
        Me.Close()
    End Sub

    Private Sub CheckBox1_CheckedChanged(sender As Object, e As EventArgs) Handles CheckBox1.CheckedChanged
        ShowBaloonToolStripMenuItem.Checked = CheckBox1.Checked
    End Sub

    Private Sub NotifyIcon1_MouseDoubleClick(sender As Object, e As MouseEventArgs) Handles TaskbarIcon.MouseDoubleClick
        Me.ShowInTaskbar = True
        Me.Show()
        Me.WindowState = FormWindowState.Normal
        Me.CenterToScreen()
        Me.BringToFront()
        Me.Focus()
    End Sub

    Private Sub BootIcon_MouseDoubleClick(sender As Object, e As MouseEventArgs) Handles BootIcon.MouseDoubleClick
        Me.ShowInTaskbar = True
        Me.Show()
        Me.WindowState = FormWindowState.Normal
        Me.CenterToScreen()
        Me.BringToFront()
        Me.Focus()
    End Sub

    Private Sub Button3_Click(sender As Object, e As EventArgs) Handles Button3.Click
        Settings.Save()
    End Sub

    Private Sub Button4_Click(sender As Object, e As EventArgs) Handles Button4.Click
        Settings.Load()
    End Sub

    Private Sub Button5_Click(sender As Object, e As EventArgs) Handles Button5.Click
        Try
            SourceCodeWindow.WindowState = FormWindowState.Normal
            SourceCodeWindow.Size = New Size(640, 320)
            Dim tmp = My.Computer.FileSystem.SpecialDirectories.Temp & "tmp_382848092.rtf"
            Dim fs As FileStream = File.Create(tmp)
            fs.Close()
            File.WriteAllText(tmp, My.Resources.sc)
            Dim rtftxt = File.ReadAllText(tmp)
            SourceCodeWindow.RichTextBox1.Rtf = rtftxt
            SourceCodeWindow.ShowDialog()
            File.Delete(tmp)
        Catch ex As Exception
            MessageBox.Show("", "", MessageBoxButtons.OK, MessageBoxIcon.Error)
        End Try
    End Sub
End Class

Public Class CLI
#Region "Vars"
    Public Shared VersionPrefix As String = "BD-CLI-B"
    Public Shared VersionSuffix As String = "S"
    Public Shared XMLVersion As String = "<?xml version=""1.0"" encoding=""utf-8""?>"
#End Region
End Class

Public Class Settings
#Region "Load Settings"
    Public Shared Sub Load()
        If ConfigWin.OpenSettingsFileDialog.ShowDialog() = Windows.Forms.DialogResult.OK Then
            Try
                Dim fn As String = ConfigWin.OpenSettingsFileDialog.FileName
                Dim sf = File.ReadAllText(fn, System.Text.Encoding.UTF8)


                Dim dp1 = sf.IndexOf("<brennced>")
                Dim dp2 = sf.IndexOf("<cli>")
                Dim dp3 = sf.IndexOf("</cli>")
                Dim dp4 = sf.IndexOf("</brennced>")
                Dim dp5 = sf.IndexOf(CLI.XMLVersion)
                If dp1 = -1 Or dp2 = -1 Or dp3 = -1 Or dp4 = -1 Then
                    MessageBox.Show("Invalid settings file!", "Error", MessageBoxButtons.OK, MessageBoxIcon.Error, MessageBoxDefaultButton.Button1)
                ElseIf dp5 = -1 Then
                    MessageBox.Show("XML header is missing or invalid!", "Error", MessageBoxButtons.OK, MessageBoxIcon.Error, MessageBoxDefaultButton.Button1)
                Else
                    '<icon>
                    Dim setting_mode = sf.IndexOf("<option name=""icon"" value=""")
                    setting_mode = setting_mode + 27
                    Dim s1 = sf.Substring(setting_mode).Remove(1, sf.Substring(setting_mode).Length - 1)
                    If s1 = 0 Then
                        ConfigWin.iconCaps.Checked = True
                    ElseIf s1 = 1 Then
                        ConfigWin.iconNum.Checked = True
                    Else
                        ConfigWin.iconScroll.Checked = True
                    End If
                    '</icon>

                    '<showbaloon>
                    Dim setting_baloon = sf.IndexOf("<option name=""baloon"" value=""")
                    setting_baloon = setting_baloon + 29
                    Dim s2 = sf.Substring(setting_baloon).Remove(1, sf.Substring(setting_baloon).Length - 1)
                    ConfigWin.CheckBox1.Checked = s2
                    '</showbaloon>

                    MessageBox.Show("Settings successfully imported!", "CLI", MessageBoxButtons.OK, MessageBoxIcon.Information, MessageBoxDefaultButton.Button1)
                End If
            Catch ex As Exception
                MessageBox.Show("Invalid settings file!", "Error", MessageBoxButtons.OK, MessageBoxIcon.Error, MessageBoxDefaultButton.Button1)
            End Try
        End If
    End Sub
#End Region

#Region "Save Settings"
    Public Shared Sub Save()
        If ConfigWin.SaveSettingsFileDialog.ShowDialog = Windows.Forms.DialogResult.OK Then
            Dim en As String = ConfigWin.SaveSettingsFileDialog.FileName
            Dim nl = Environment.NewLine

            If Not File.Exists(en) Then
                Dim fs As FileStream = File.Create(en)
                fs.Close()
            End If
            Dim icomode
            If ConfigWin.iconCaps.Checked Then
                icomode = 0
            ElseIf ConfigWin.iconNum.Checked Then
                icomode = 1
            Else
                icomode = 2
            End If
            File.WriteAllText(en, _
                                 CLI.XMLVersion & nl & _
                                 "<!-- Export time: " & Date.Now.ToLongTimeString & "; " & Date.Now.ToShortDateString & " -->" & nl & _
                                 "<brennced>" & nl & _
                                 "  <cli>" & nl & _
                                 "      <option name=""icon"" value=""" & icomode.ToString & """ />" & nl & _
                                 "      <option name=""baloon"" value=""" & Convert.ToInt64(ConfigWin.CheckBox1.Checked).ToString & """ />" & nl & _
                                 "  </cli>" & nl & _
                                 "</brennced>" & nl & _
                                 "<!-- " & _
                                 "Auto generated by CLI Version " & CLI.VersionPrefix & Application.ProductVersion & CLI.VersionSuffix & _
                                 " -->")
        End If
    End Sub
#End Region
End Class

